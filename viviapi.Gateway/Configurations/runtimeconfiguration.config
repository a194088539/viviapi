    <!--实现MSMQ对队的程序集-->
    <add key="OrderMessaging" value="viviapi.MSMQMessaging"></add>
    <!-- Batch Processing options -->
    <add key="ThreadCount" value="2" />
    <add key="BatchSize" value="10" />
    <add key="QueueTimeout" value="20" />
